<h1>Events#Index</h1>
<div class="container">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">User #</th>
          <th scope="col">Title</th>
          <th scope="col">Creator</th>
          <th scope="col">Start Time</th>
          <th scope="col">End Time</th>
        </tr>
      </thead>
      <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.user.id %></td>
        <td><%= event.title %></td>
        <td><%= event.user.name %></td>
        <td><%= event.start_time %></td>
        <td><%= event.end_time %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
  <%console%>
</div>