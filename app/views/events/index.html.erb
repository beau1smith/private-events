<h1>Events#index</h1>
<p>Find me in app/views/events/index.html.erb</p>

<% if user_signed_in? %>
  <div class="container">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Title</th>
          <th scope="col">Start Time</th>
          <th scope="col">End Time</th>
        </tr>
      </thead>
      <tbody>
    <% current_user.events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.start_time %></td>
        <td><%= event.end_time %></td>
      </tr>
    <% end %>
<% end%>
    </tbody>
  </table>
</div>
<% console %>
